{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "asset-registry": {
        "ClusterId": "asset-registry",
        "Match": { "Path": "/assetregistry/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/assetregistry" } ]
      },
      "audit-compliance": {
        "ClusterId": "audit-compliance",
        "Match": { "Path": "/audit/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/audit" } ]
      },
      "identity-access": {
        "ClusterId": "identity-access",
        "Match": { "Path": "/identity/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/identity" } ]
      },
      "maintenance-scheduler": {
        "ClusterId": "maintenance-scheduler",
        "Match": { "Path": "/maintenance/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/maintenance" } ]
      },
      "notification": {
        "ClusterId": "notification",
        "Match": { "Path": "/notification/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/notification" } ]
      }
    },
    "Clusters": {
      "asset-registry": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5237/" }
        }
      },
      "audit-compliance": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5288/" }
        }
      },
      "identity-access": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5301/" }
        }
      },
      "maintenance-scheduler": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5195/" }
        }
      },
      "notification": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5079/" }
        }
      }
    }
  }
  ,
  "Authentication": {
    "Authority": "",
    "Audience": ""
  }
}
